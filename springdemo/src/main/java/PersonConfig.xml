<?xml version="1.0" encoding="UTF-8"?>

<beans
xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="person1"
		class="com.coforge.training.springdemo.model.Person">

	<!-- Constructor Injection -->
	<constructor-arg value="999" type="int"></constructor-arg>
	<constructor-arg value="Patrick"></constructor-arg>
	<constructor-arg>
		<map>
			<entry key="mobile" value="989799890"></entry>
			<entry key="landline" value="012924055"></entry>
		</map>
	
	</constructor-arg>
	
		</bean>
		</beans>






