<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- creating Bean/object of Employee class -->
	<bean id="emp1"
		class="com.coforge.training.springdemo.model.Employee">


		<!-- Invokes Setter Method - setName() of student Class -->
		<!-- property tag is responsible for setter injection -->
		<property name="id" value="101"></property>
		<property name="name" value="Prateek Sharma"></property>
		<property name="city" value="Faridabad"></property>

	</bean>





</beans>
